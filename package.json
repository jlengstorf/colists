{
  "name": "colists",
  "version": "0.0.1",
  "description": "CoLists is a simple app to demonstrate the concept of real-time collaboration while using the Flux-style data flow.",
  "main": "src/index.html",
  "scripts": {
    "start": "./node_modules/.bin/webpack-dev-server --port 3000 --content-base=./public --history-api-fallback",
    "build": "BUILD_ENV=production webpack --progress --colors --optimize-minimize --optimize-dedupe",
    "deploy": "echo \"TODO\" # NODE_ENV=production npm run build && git add dist/* && git commit -m 'build(deploy)' && git push heroku develop:master",
    "sw": "babel src/js/service-workers/offline-caching.js -o public/sw.js --no-comments --compact true",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jlengstorf/colists.git"
  },
  "author": "Jason Lengstorf <jason@lengstorf.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jlengstorf/colists/issues"
  },
  "homepage": "https://github.com/jlengstorf/colists#readme",
  "devDependencies": {
    "babel-core": "^6.2.1",
    "babel-jest": "^6.0.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "css-loader": "^0.23.0",
    "cssnano": "^3.3.2",
    "cssnext": "^1.8.4",
    "jest-cli": "^0.8.0",
    "jest-webpack-alias": "^2.0.0",
    "jscs": "^2.6.0",
    "jscs-loader": "^0.2.0",
    "postcss-import": "^7.1.3",
    "postcss-loader": "^0.8.0",
    "postcss-nested": "^1.0.0",
    "postcss-simple-vars": "^1.1.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs",
      "<rootDir>/node_modules/flux",
      "<rootDir>/node_modules/classnames"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/node_modules"
    ],
    "verbose": true
  },
  "dependencies": {
    "debug": "^2.2.0",
    "events": "^1.1.0",
    "getconfig": "^2.2.0",
    "immutable": "^3.7.5",
    "mongodb": "^2.0.52",
    "socket.io": "^1.3.7",
    "socket.io-client": "^1.3.7"
  }
}
